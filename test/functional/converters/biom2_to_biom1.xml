<tool id="test_CONVERTER_biom2_to_biom1" name="Test Convert Biom2 to Biom1" version="0.1">
    <command><![CDATA[
        cp '$biom1' '$output_biom1'
    ]]></command>
    <inputs>
        <param name="biom1" type="data" format="biom1" label="Biom1 file"/>
    </inputs>
    <outputs>
        <data name="output_biom1" format="biom1"/>
    </outputs>
    <tests>
        <test>
            <param name="biom1" ftype="biom2" value="input_taxonomy.biom2"/>
            <output name="output_biom1">
                <assert_contents>
                    <has_text text="Biological Observation Matrix 1.0.0"/>
                </assert_contents>
            </output>
        </test>
    </tests>
    <help>
    </help>
</tool>
