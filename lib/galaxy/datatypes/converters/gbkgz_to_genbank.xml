<tool id="CONVERTER_gbkgz_to_genbank" name="Convert gbk.gz files to genbank" version="1.0.0" hidden="true">
  <command>gzip -dcf '$input1' > '$output1'</command>
  <inputs>
    <param format="gbk.gz" name="input1" type="data" label="Choose Gbk file"/>
  </inputs>
  <outputs>
    <data format="genbank" name="output1"/>
  </outputs>
  <help>
  </help>
</tool>
