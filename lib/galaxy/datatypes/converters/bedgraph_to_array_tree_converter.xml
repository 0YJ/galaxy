<tool id="CONVERTER_BedGraph_0" name="Index BedGraph for Track Viewer" version="1.0.1" hidden="true" profile="16.04">
    <!-- Used internally to generate track indexes -->
    <requirements>
        <requirement type="package" version="0.8.6">bx-python</requirement>
    </requirements>
    <command>python '$__tool_directory__/bedgraph_to_array_tree_converter.py' '$input' '$output'</command>
    <inputs>
        <param format="bedgraph" name="input" type="data" label="Choose BedGraph"/>
    </inputs>
    <outputs>
        <data format="array_tree" name="output"/>
    </outputs>
    <tests>
        <test>
            <param name="input" format="bedgraph" value="gtf2bedgraph_out.bedgraph"/>
            <output name="output" format="array_tree" value="gtf2array_tree_out.array_tree"/>
        </test>
    </tests>
    <help>
    </help>
</tool>
