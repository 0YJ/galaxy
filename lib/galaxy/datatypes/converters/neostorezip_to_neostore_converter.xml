<tool id="CONVERTER_neostorezip_to_neostore" name="Convert neostore.zip files to neostore" version="1.0.0" hidden="true">
  <command>unzip '$input1' -d '$output1.files_path' > $output1</command>
  <inputs>
    <param format="neostore.zip" name="input1" type="data" label="Choose neostore database file"/>
  </inputs>
  <outputs>
    <data format="neostore" name="output1"/>
  </outputs>
  <help>
  </help>
</tool>
